{
  "name": "heimrichhannot/contao-choices-bundle",
  "type": "contao-bundle",
  "description": "This bundle offers support for choices.js for the Contao CMS.",
  "license": "LGPL-3.0-or-later",
  "version": "0.1",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "contao/core-bundle": "^4.9",
    "heimrichhannot/contao-utils-bundle": "^2.196",
    "psr/container": "^1.0|^2.0",
    "symfony/dependency-injection": "^3.4|^4.4"
  },
  "suggest": {
    "heimrichhannot/contao-encore-bundle": "^1.5 - Manage dependencies with Symfony Webpack Encore."
  },
  "conflict": {
    "heimrichhannot/contao-encore-bundle": "<1.5"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\ChoicesBundle\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\ChoicesBundle\\ContaoManager\\Plugin",
    "foxy": true
  }
}
